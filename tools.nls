to move-hw-manager[ hw mng]
  
  let yc 0
  let xc 0
  
  ask homework hw[
    set yc ycor
    set xc xcor
    ]
  
  ask administrator mng [    
    set xcor xc - 1 
    set ycor yc - 1
    ]
  
 
end


